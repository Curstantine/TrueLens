{
	"editor.defaultFormatter": "esbenp.prettier-vscode",
	"[prisma]": {
		"editor.defaultFormatter": "Prisma.prisma"
	},
	"cSpell.words": [
		"derana",
		"factualize",
		"factualized",
		"Factualizing",
		"GROQ",
		"hiru",
		"iconify",
		"jabascript",
		"pipenv",
		"sonner",
		"tabler",
		"waitlist"
	],
	"files.associations": {
		"*.css": "tailwindcss"
	},
	"tailwindCSS.experimental.classRegex": [
		["cva\\(((?:[^()]|\\([^()]*\\))*)\\)", "[\"'`]([^\"'`]*).*?[\"'`]"],
		["cx\\(((?:[^()]|\\([^()]*\\))*)\\)", "(?:'|\"|`)([^']*)(?:'|\"|`)"]
	],
	"git.ignoredRepositories": ["news_filtered_data", "news_source_data"]
}
